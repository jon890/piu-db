  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "@prisma/client": "^5.7.0",
    "autoprefixer": "10.4.17",
    "bcrypt": "^5.1.1",
    "dayjs": "^1.11.10",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "ky": "^1.2.0",
    "next": "14.1.0",
    "next-auth": "5.0.0-beta.13",
    "postcss": "8.4.35",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.33.2",
    "tailwindcss": "3.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "20.11.19",
    "@types/react": "18.2.57",
    "@types/react-dom": "18.2.19",
    "daisyui": "^4.7.2",
    "dotenv-cli": "^7.3.0",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  }
}
