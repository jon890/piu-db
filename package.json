{
  "name": "piu-db",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db-push:local": "dotenv -e .env.local -- npx prisma migrate dev --name $name",
    "db-push:dev": "dotenv -e .env.development -- npx prisma migrate dev --name $name",
    "db:generate": "prisma generate",
    "db-init:local": "dotenv -e .env.local -- ts-node ./db/init_songs",
    "db-init:dev": "dotenv -e .env.development -- ts-node ./db/init_songs"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@prisma/client": "^5.7.0",
    "autoprefixer": "10.4.16",
    "bcrypt": "^5.1.1",
    "eslint": "8.55.0",
    "eslint-config-next": "14.0.4",
    "ky": "^1.1.3",
    "next": "14.0.4",
    "next-auth": "5.0.0-beta.3",
    "postcss": "8.4.32",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "20.10.4",
    "@types/react": "18.2.43",
    "@types/react-dom": "18.2.17",
    "daisyui": "^4.4.19",
    "dotenv-cli": "^7.3.0",
    "prisma": "^5.7.0",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  }
}
